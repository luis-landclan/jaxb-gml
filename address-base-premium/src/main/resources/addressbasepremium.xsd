  <element name="Street" substitutionGroup="abpr:FeatureWithLifecycle" type="abpr:StreetType"/>
  <complexType name="StreetType">
    <complexContent>
      <extension base="abpr:FeatureWithLifecycleType">
        <sequence>
          <element name="usrn" type="integer">
            <annotation>
              <documentation>Unique Street Reference Number (USRN) - the unique key for the record
                and primary key for the Street table.</documentation>
            </annotation>
          </element>
          <element name="recordType" type="abpr:StreetRecordTypeCodeType">
            <annotation>
              <documentation>The record type of the street, for example a ‘Numbered
                Street’.</documentation>
            </annotation>
          </element>
          <element name="swaOrgRefNaming" type="integer">
            <annotation>
              <documentation>The code which identifies the Street Naming and Numbering Authority or
                the Local Highway Authority.</documentation>
            </annotation>
          </element>
          <element minOccurs="0" name="state" type="abpr:StreetStateCodeType">
            <annotation>
              <documentation>A code identifying the current state of the Street, ‘Open’ for
                example.</documentation>
            </annotation>
          </element>
          <element minOccurs="0" name="stateDate" type="date">
            <annotation>
              <documentation>Date at which the street achieved its current state in the world as
                referenced to the ‘State’ column.</documentation>
            </annotation>
          </element>
          <element minOccurs="0" name="streetSurface" type="abpr:StreetSurfaceCodeType">
            <annotation>
              <documentation>A code to indicate the surface finish of the street.</documentation>
            </annotation>
          </element>
          <element minOccurs="0" name="streetClassification"
            type="abpr:StreetClassificationCodeType">
            <annotation>
              <documentation>A code for the primary street classification, for example denoting it
                to be ‘open to all vehicles’.</documentation>
            </annotation>
          </element>
          <element name="version" type="integer">
            <annotation>
              <documentation>Version number of the street record.</documentation>
            </annotation>
          </element>
          <element name="streetStart" type="gml:PointPropertyType" nillable="true">
            <annotation>
              <documentation>A value in metres defining the x and y location in accordance to the
                British National Grid for the start point of the street.</documentation>
            </annotation>
          </element>
          <element name="streetStartLatLong" type="gml:PointPropertyType">
            <annotation>
              <documentation>A value defining the Latitude and Longitude start point of the street
                in accordance with the ETRS89 coordinate reference system.</documentation>
            </annotation>
          </element>
          <element name="streetEnd" type="gml:PointPropertyType" nillable="true">
            <annotation>
              <documentation>A value in metres defining the x and y location in accordance to the
                British National Grid for the end point of the street.</documentation>
            </annotation>
          </element>
          <element name="streetEndLatLong" type="gml:PointPropertyType">
            <annotation>
              <documentation>A value defining the Latitude and Longitude end point of the street in
                accordance with the ETRS89 coordinate reference system.</documentation>
            </annotation>
          </element>
          <element name="streetTolerance" type="integer">
            <annotation>
              <documentation>The tolerance of the start and end coordinates in
                metres.</documentation>
            </annotation>
          </element>
          <element maxOccurs="unbounded" name="streetDescriptiveIdentifierMember"
            type="abpr:StreetDescriptiveIdentifierPropertyType"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
